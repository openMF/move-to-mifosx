  <step>
    <name>Read Mifos Customer Attendance</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>SELECT
	ca.id AS s_attendance_id,
	ca.meeting_date AS s_meeting_date,
	ca.customer_id AS s_customer_id,
	ca.attendance AS s_attendance_code,
	cent.customer_id AS s_center_id
FROM
	customer_attendance ca
	LEFT JOIN customer c
		ON ca.customer_id = c.customer_id AND
		c.discriminator = &apos;CLIENT&apos; AND
		c.branch_id = 12
	LEFT JOIN customer grp
		ON c.parent_customer_id = grp.customer_id AND
		grp.discriminator = &apos;GROUP&apos; AND
		grp.branch_id = 12
	LEFT JOIN customer cent
		ON grp.parent_customer_id = cent.customer_id AND
		cent.discriminator = &apos;CENTER&apos; AND
		cent.branch_id = 12
</sql>
    <limit>0</limit>
