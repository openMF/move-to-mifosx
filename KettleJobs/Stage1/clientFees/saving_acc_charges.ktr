  <step>
    <name>Input Account fees</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>SELECT
 cr.CUSTOMER_ID AS client_id,
 af.ACCOUNT_FEE_ID As fee_account_id,
 af.FEE_AMNT AS amount,
 if(af.FEE_STATUS=2,0,1) AS is_active,
 af.FEE_ID As charge_id,
 ifnull(ftd.FEE_AMOUNT,0) AS amount_paid_derived,
 ifnull(af.FEE_AMNT,0) - ifnull(ftd.FEE_AMOUNT,0) AS amount_outstanding_derived,
 IF(ifnull(af.FEE_AMNT,0)-ifnull(ftd.FEE_AMOUNT,0)=0,1,0) AS is_paid_derived,
 curdate() AS charge_due_date, -- not used, instead derived in next step
 atx.ACTION_DATE As transaction_date,
 cr.BRANCH_ID As office_id,
 atx.ACCOUNT_TRXN_ID as transaction_id
FROM account_fees af
INNER JOIN fees f ON af.FEE_ID=f.FEE_ID AND f.CATEGORY_ID = 2 -- 2 =&gt; Client fees 
INNER JOIN account ac ON ac.ACCOUNT_ID=af.ACCOUNT_ID 
INNER JOIN customer cr ON ac.CUSTOMER_ID = cr.CUSTOMER_ID AND ac.ACCOUNT_TYPE_ID = 3 AND cr.DISCRIMINATOR = &apos;CLIENT&apos;  
LEFT JOIN fee_trxn_detail ftd ON af.ACCOUNT_FEE_ID=ftd.ACCOUNT_FEE_ID
LEFT JOIN account_trxn atx ON ftd.ACCOUNT_TRXN_ID=atx.ACCOUNT_TRXN_ID</sql>
    <limit>0</limit>
    <lookup/>
