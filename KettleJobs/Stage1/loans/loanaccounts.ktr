    <sql>SELECT &#xd;&#xa;  la.ACCOUNT_ID As 	id&#xd;&#xa;, la.ACCOUNT_ID As 	account_no&#xd;&#xa;, ac.GLOBAL_ACCOUNT_NUM As 	external_id&#xd;&#xa;, ac.CUSTOMER_ID As 	client_id&#xd;&#xa;, IF &#x28; cr.status_id &#x3d; 3 , cr.PARENT_CUSTOMER_ID , null &#x29; As group_id&#xd;&#xa;, la.PRD_OFFERING_ID As 	product_id&#xd;&#xa;, la.FUND_ID As 	fund_id&#xd;&#xa;, cr.loan_officer_id As 	loan_officer_id&#xd;&#xa;, la.BUSINESS_ACTIVITIES_ID As 	loanpurpose_cv_id&#xd;&#xa;, ac.ACCOUNT_STATE_ID As 	loan_status_id&#xd;&#xa;, if &#x28; la.GROUP_FLAG &#x3d; 1 , 2 , 1 &#x29; As 	loan_type_enum&#xd;&#xa;, &#x24;&#x7b;CUR&#x7d; As 	currency_code&#xd;&#xa;, &#x24;&#x7b;CURDIGITS&#x7d; As 	currency_digits&#xd;&#xa;, la.LOAN_AMOUNT As 	principal_amount&#xd;&#xa;, NULL As 	arrearstolerance_amount&#xd;&#xa;, IFNULL&#x28;la.INTEREST_RATE , 0.000000 &#x29; As nominal_interest_rate_per_period&#xd;&#xa;, 3 As 	interest_period_frequency_enum  -- Per year&#xd;&#xa;, IFNULL&#x28;la.INTEREST_RATE , 0.000000 &#x29;  As 	annual_nominal_interest_rate&#xd;&#xa;, la.INTEREST_TYPE_ID As 	interest_method_enum&#xd;&#xa;, 1 As 	interest_calculated_in_period_enum  -- same as repayment freq period&#xd;&#xa;, la.NO_OF_INSTALLMENTS As 	term_frequency&#xd;&#xa;, rd.RECURRENCE_ID As 	term_period_frequency_enum&#xd;&#xa;, rd.RECUR_AFTER As 	repay_every&#xd;&#xa;, rd.RECURRENCE_ID As 	repayment_period_frequency_enum&#xd;&#xa;, la.NO_OF_INSTALLMENTS As 	number_of_repayments&#xd;&#xa;, IF&#x28;la.GRACE_PERIOD_TYPE_ID &#x3d; 2 OR la.GRACE_PERIOD_TYPE_ID &#x3d; 3 , la.GRACE_PERIOD_DURATION , NULL  &#x29; As 	grace_on_principal_periods&#xd;&#xa;, IF&#x28;la.GRACE_PERIOD_TYPE_ID &#x3d; 2 , la.GRACE_PERIOD_DURATION , NULL  &#x29; As 	grace_on_interest_periods&#xd;&#xa;, NULL As 	grace_interest_free_periods&#xd;&#xa;, 1    As 	amortization_method_enum&#xd;&#xa;, ac.CREATED_DATE As 	submittedon_date&#xd;&#xa;, ac.CREATED_BY As 	submittedon_userid&#xd;&#xa;, NULL As 	approvedon_date&#xd;&#xa;, NULL As 	approvedon_userid&